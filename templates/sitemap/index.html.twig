<?xml version="1.0" encoding="UTF-8"?>

<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:image="http://www.google.com/schemas/sitemap-image/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 https://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">


{% for url in urls %}
    <loc>
        {% if url.loc|replace({hostname:''}) == url.loc %}
            {{ hostname }}{{ url.loc }}
        {% else %}
            {{ url.loc }}
        {% endif %}
    </loc>
{% endfor %}

</urlset>